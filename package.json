{
  "name": "express-babel",
  "version": "1.0.0",
  "description": "Using latest JavaScript features on the server",
  "repository": "https://github.com/vmasto/express-babel",
  "author": "Vassilis Mastorostergios <vmasto@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": "~6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon --inspect=22688 src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.4.0",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "express-jwt": "^5.3.0",
    "googleapis": "^22.2.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.31",
    "mongoose": "^4.12.1",
    "mongoose-actor": "0.0.2",
    "mongoose-autopopulate": "^0.6.0",
    "mongoose-delete": "^0.4.0",
    "mongoose-hidden": "^1.5.0",
    "mongoose-query": "^0.5.1",
    "morgan": "^1.7.0",
    "node-emoji": "^1.8.1",
    "throw.js": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-register": "^6.24.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.0",
    "jest": "^21.0.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "node",
      "promise",
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "standard"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2,
      "no-console": 0,
      "no-unused-vars": [
        2,
        {
          "argsIgnorePattern": "next"
        }
      ]
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
